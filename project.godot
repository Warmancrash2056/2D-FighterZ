run/main_scene="res://Game Maps/Training/Training Galvin.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/image="res://Character Resouces/Hunter/Icon/Hunter.png"
